{
  "root": true,
  "env": {
      "browser": true,
      "commonjs": true,
      "es2020": true,
      "node": true,
      "jest": true
  },
  "extends": "trybe-backend",
  "rules": {
    "complexity": [
      true,
      5,
      {
        "ignoredClauses": [
          "caseClause"
        ]
      }
    ]
  },
  "parserOptions": { "ecmaVersion": 11 },
  "globals":{
    "db": "writable"
  }
}
